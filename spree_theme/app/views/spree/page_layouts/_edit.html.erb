<% #edit page_layout in editor, 
   # title, columns, data_resource, 
%>
<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @page_layout } %>
<%=form_for  @page_layout, url: template_theme_page_layout_path(@template_theme, @page_layout), remote: true do |f| %>
<div>
    <%= f.label :title %>
    <%= f.text_field :title %>
</div>
<div>
  <%= f.submit Spree.t('update') %> <%= f.button Spree.t('cancel'), type: :button, class: 'cancel'%>
  
</div>

<% end %>
